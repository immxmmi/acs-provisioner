# ACS Provisioner Configuration
# Copy this file to .env and configure your values

# ACS Central Connection
API_HOST=central.example.com
API_PORT=443
API_BASE_PATH=/
API_AUTH_TYPE=bearer
API_TOKEN=your-acs-api-token-here

# TLS Settings
DISABLE_TLS_VERIFY=false
CA_BUNDLE=

# Debug Settings
DEBUG_ENABLED=false
SHOW_CURL=false

# Pipeline Configuration
PIPELINE_FILE=pipelines/pipeline.yaml
INPUTS_FILE=pipelines/inputs.yaml
